<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.alibaba.cloud.mapper.UserMapper">

    <select id="loadUserByUsername" resultType="com.alibaba.cloud.pojo.User" parameterType="java.lang.String">
        select * from user where username = #{username}
    </select>
</mapper>